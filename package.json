  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "standard": "^8.6.0"
  },
  "peerDependencies": {
    "eslint-config-standard": "^6.2.1"
  },
  "standard": {
    "env": {
      "mocha": true
    }
  }
